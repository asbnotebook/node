<main role="main" class="container">
    <h1 class="mt-5">My Website</h1>
    <h2>My subscribers list</h2>
    <table class="table">
        <thead>
            <tr>
                <th scope="col">Name</th>
                <th scope="col">Email</th>
            </tr>
        </thead>
        <tbody>
            <% mySubscribers.forEach(subscriber=> { %>
                <tr>
                    <td>
                        <%= subscriber.name %>
                    </td>
                    <td>
                        <%= subscriber.email %>
                    </td>
                </tr>
            <% }); %>
    </table>
    </table>
</main>